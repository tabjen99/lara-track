{
    "name": "tabjen99/lara-track",
    "description": "This package helps to create cards in trello board when system exception found.",
    "type": "library",
    "require": {
        "gregoriohc/laravel-trello": "^0.3.0"
    },
    "require-dev": {
        "gregoriohc/laravel-trello": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
          "miketan\\laraTrack\\": "src/"
        }
    },
    "authors": [
        {
            "name": "miketan",
            "email": "tabjen99@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "miketan\\laraTrack\\laraTrackServiceProvider"
            ],
            "aliases": {
                "TrelloCrashReport": "miketan\\laraTrack\\CrashReportFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
